#include <iostream>
#include <vector>

using namespace std;

int main()
{
	//setlocale(LC_CTYPE, "ukr");

	int n, t;
	//cout<<"Введiть розмiр першого i другого масивiв: ";
	cin >> n >> t;
	int a[n];
	int arr[t];
	//cout<<"Введiть перший масив";
	for (int i = 0; i < n; ++i)
	{
		cin >> a[i];
	}
		//cout<<"Введiть другий масив";

	for (int i = 0; i < t; ++i)
	{

		cin >> arr[i];
	}


	for(int i = 0; i < t; i++)
	{
		int l = 0, r = n - 1;
		int res1 = -1;
		int res2 = -1;
		while (l <= r)
		{
			int m = (l + r) / 2;
			if (a[m] == arr[i])
			{
				res1 = m;
				r = m - 1;
			}
			else if (a[m] > arr[i])
			{
				r = m - 1;
			}
			else
			{
				l = m + 1;
			}
		}
		
		l = 0, r = n - 1;
		
		while (l <= r)
		{
			int m = (l + r) / 2;
			if (a[m] == arr[i])
			{
				res2 = m;
				l = m + 1;
			}
			else if (a[m] > arr[i])
			{
				r = m - 1;
			}
			else
			{
				l = m + 1;
			}
		}
		if (res1 != -1 && res2 != -1)
		{
			res1++;
			res2++;
			cout << res1 << " " << res2 << endl;
		}
		else
		{
			cout << "0";
			cout << endl;
		}

	}	

}
