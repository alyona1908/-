int binary_search(int* array, int arr_size, int element)
{
    int left = 0;            
    int right = arr_size - 1;    
 
    while (left <= right) 
    {
 
        int m = (left + right) / 2;
 
        if (array[m] == element) return m; // if m == key return key
 
        if (array[m] < element) left = m + 1; // if m < key -> search in right
 
        if (array[m] > element) right = m - 1; // if m > key -> serarch in left
 
    }
 
  return -1;
}
